<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.calculator.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" prefHeight="7.0" prefWidth="0.0" />
   <TextField fx:id="txtfield" prefHeight="19.0" prefWidth="223.0" />
   <GridPane prefHeight="223.0" prefWidth="223.0">
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
         <Button fx:id="one" mnemonicParsing="false" onAction="#Number" prefHeight="48.0" prefWidth="75.0" text="1">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button fx:id="two" mnemonicParsing="false" onAction="#Number" prefHeight="50.0" prefWidth="75.0" text="2" GridPane.columnIndex="1">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button fx:id="three" mnemonicParsing="false" onAction="#Number" prefHeight="46.0" prefWidth="93.0" text="3" GridPane.columnIndex="2">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button fx:id="four" mnemonicParsing="false" onAction="#Number" prefHeight="50.0" prefWidth="75.0" text="4" GridPane.rowIndex="1">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button fx:id="five" mnemonicParsing="false" onAction="#Number" prefHeight="46.0" prefWidth="86.0" text="5" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button fx:id="six" mnemonicParsing="false" onAction="#Number" prefHeight="46.0" prefWidth="88.0" text="6" GridPane.columnIndex="2" GridPane.rowIndex="1">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button fx:id="seven" mnemonicParsing="false" onAction="#Number" prefHeight="46.0" prefWidth="87.0" text="7" GridPane.rowIndex="2">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button fx:id="eight" mnemonicParsing="false" onAction="#Number" prefHeight="46.0" prefWidth="84.0" text="8" GridPane.columnIndex="1" GridPane.rowIndex="2">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button fx:id="nine" mnemonicParsing="false" onAction="#Number" prefHeight="46.0" prefWidth="95.0" text="9" GridPane.columnIndex="2" GridPane.rowIndex="2">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button fx:id="zero" mnemonicParsing="false" onAction="#Number" prefHeight="46.0" prefWidth="86.0" text="0" GridPane.rowIndex="3">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button fx:id="add" mnemonicParsing="false" onAction="#Operation" prefHeight="46.0" prefWidth="103.0" text="+" GridPane.columnIndex="1" GridPane.rowIndex="3">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button fx:id="sub" mnemonicParsing="false" onAction="#Operation" prefHeight="46.0" prefWidth="127.0" text="-" GridPane.columnIndex="2" GridPane.rowIndex="3">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button fx:id="mult" mnemonicParsing="false" onAction="#Operation" prefHeight="46.0" prefWidth="82.0" text="*" GridPane.rowIndex="4">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button fx:id="div" mnemonicParsing="false" onAction="#Operation" prefHeight="51.0" prefWidth="75.0" text="/" GridPane.columnIndex="1" GridPane.rowIndex="4">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button fx:id="eq" mnemonicParsing="false" onAction="#Operation" prefHeight="46.0" prefWidth="86.0" text="=" GridPane.columnIndex="2" GridPane.rowIndex="4">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
      </children>
   </GridPane>
</VBox>
